{
  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "javascript",
    "less",
    "sass"
  ],
  "eslint.nodePath": "/usr/local/lib/node_modules",
  "eslint.autoFixOnSave" : true,
  "eslint.packageManager" : "npm"
}
